    width: 96%;
  }

  #credit_card_verification_value { margin-left: 6px; }

  .plisthead {
    font-size: 11px !important;
    line-height: 16px;
    font-family: Helvetica, Arial, sans-serif;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
