    /* Body reset
    *  -------------------------*/
    html {
        font-size: 62.5%;
        -webkit-tap-highlight-color: rgba(0,0,0,0);
    }

    body {
        font-family: {{ font_standard_type }}, "Helvetica Neue", Helvetica, Arial, sans-serif;
        font-style: {{ settings.font_standard_style }};
        font-size: {{ settings.font_standard_size }}px;
        line-height: 1.428571429;
        color:{{ settings.color_body_standard }};
        background:{{ settings.color_bg_header }};
    }

    .bodywrap {
        background:{{ settings.color_bg_main }}
        {% unless settings.bg_image_body == '' %}
            {% if settings.bg_offset_body_y == '' %}
                {% assign bg_offset_body_y = '0px' %}
            {% else %}
                {% capture bg_offset_body_y %}{{ settings.bg_offset_body_y }}px{% endcapture %}
            {% endif %}
            url('{{ settings.bg_image_body | asset_url }}') {{ settings.bg_repeat_body | replace: 'top', bg_offset_body_y }}
        {% endunless %};
        position: relative;
        z-index: 5;
        {% if settings.head_foot_border == true %}
        border-top: {{ settings.color_general_divider }} 1px solid;
        {% else %}
        border-top: none;
        {% endif %}
    }

    body > .bodywrap {
        position: relative;
        z-index: 50;
    }

    /* Links
    *  -------------------------*/

    a {
        color: {{ settings.color_body_link }};
        text-decoration: none;
    }

    a.highlight {
        display: inline-block;
        zoom: 1;
        *display: inline;
        max-width: 100%;
           -moz-transition: box-shadow 200ms; /* Firefox 4 */
        -webkit-transition: box-shadow 200ms; /* Safari and Chrome */
             -o-transition: box-shadow 200ms; /* Opera */
                transition: box-shadow 200ms;
    }

    a.highlight img {
        z-index: -1;
        position: relative;
    }

    h1, h2, h3, h4, h5, h6 {
        font-family: {{ font_heading_type }}, "Helvetica Neue", Helvetica, Arial, sans-serif;
        font-size:   {{ settings.font_heading_size }}px;
        font-style:  {{ settings.font_heading_style }};
        line-height: 1.1;
    }

    h2 {
        font-size: {{ settings.font_heading_size | plus: -4 }}px;
    }
    h3 {
        font-size: {{ settings.font_heading_size | plus: -6 }}px;
    }
    h4 {
        font-size: {{ settings.font_heading_size | plus: -8 }}px;
    }
    h5 {
        font-size: {{ settings.font_heading_size | plus: -10 }}px;
    }
    h6 {
        font-size: {{ settings.font_heading_size | plus: -12 }}px;
    }

    input,
    .textboxcont,
    textarea {
        background: {{ settings.color_form_field_bg }};
        border: 1px solid {{ settings.color_form_field_border }};
        color: {{ settings.color_form_field_text }};
    }

    .hidden {
        display: none;
    }

    a.button,
    input.button,
    .button a {
        background: {{ settings.color_btn_bg }};
        font-family: {{ font_heading_type }}, "Helvetica Neue", Helvetica, Arial, sans-serif;
        color: {{ settings.color_btn_text }};
    }

    a.button.altcolor,
    input.button.altcolor,
    .button.altcolor a {
        background-color: {{ settings.color_btn_alt_bg }};
    }

    a.button:hover,
    input.button:hover,
    .button a:hover {
        color: {{ settings.color_btn_text }};
    }

    .betweenbuttons {
        margin-left: 10px;
        margin-right: 10px;
    }

    .loadingimg {
        background: url({{ settings.loading_graphic | asset_url }}) center center no-repeat;
        min-height: 40px;
    }





/* -------------------------  
  CHECKOUT
------------------------- */

  /* checkout pages have no bodywrap elem... so the bgimage must go directly on the body itself. */
  body {
      background:{{ settings.color_bg_main }}
          {% unless settings.bg_image_body == '' %}
              {% if settings.bg_offset_body_y == '' %}
                  {% assign bg_offset_body_y = '0px' %}
              {% else %}
                  {% capture bg_offset_body_y %}{{ settings.bg_offset_body_y }}px{% endcapture %}
              {% endif %}
              url('{{ settings.bg_image_body | asset_url }}') {{ settings.bg_repeat_body | replace: 'top', bg_offset_body_y }}
          {% endunless %};
  }

  /* progress spinners are a mess */
  #purchase-progress,
  #cost #spinner {
    display: inline-block;
    width: auto;
    background-color: #fff;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    -ms-border-radius: 5px;
    -o-border-radius: 5px;
    border-radius: 5px;
  }

