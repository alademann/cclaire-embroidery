a.button.large,
input.button.large,
.button.large a {
  padding: 10px 16px;
  text-transform: uppercase;
  font-size: 18px;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  -ms-border-radius: 6px;
  -o-border-radius: 6px;
  border-radius: 6px;
