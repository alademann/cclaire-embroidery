textarea {
  height: auto;
}

input.short,
.input-small {
  height: 34px;
  padding: 6px 12px;
  font-size: 14px;
  border-radius: 4px;
}

.textboxcont input {
  border: 0;
  height: 34px;
  width: 100%;
}

/* Hover for standard text <a> tags */
#maincontent p a,
#maincontent .content-block a,
#maincontent .contentsection a,
#maincontent .tags a,
#maincontent .info a,
#maincontent .pagesnippetwidget a,
#maincontent .recentarticles a h4,
#maincontent #basket .desc a,
#maincontent #basket .remove a {
  border-bottom: 1px dashed transparent;
  -webkit-transition: border-color 200ms linear;
  -moz-transition: border-color 200ms linear;
  -o-transition: border-color 200ms linear;
  transition: border-color 200ms linear;
}
#maincontent p a:hover,
#maincontent .content-block a:hover,
#maincontent .contentsection a:hover,
#maincontent .tags a:hover,
#maincontent .info a:hover,
#maincontent .pagesnippetwidget a:hover,
#maincontent .recentarticles a h4:hover,
#maincontent #basket .desc a:hover,
#maincontent #basket .remove a:hover {
  border-color: $color_body_link;
}

.price,
[itemprop="price"],
.amount,
.total,
#cost {
  font-family: "Droid Sans", Helvetica, Arial, sans-serif !important;
}
.price strong,
[itemprop="price"] strong,
.amount strong,
.total strong,
#cost strong {
  font-weight: 700 !important;
}

#productinfo [itemprop="price"] strong {
  font-size: $fontBigPriceSize;
  color: $color_btn_bg;
}

#cost {
  font-weight: 700 !important;
}

button::-moz-focus-inner,
select::-moz-focus-inner {
  padding: 0;
  border: 0;
}

a.button,
input.button,
.button a {
  display: inline-block;
  zoom: 1;
  *display: inline;
  padding: 6px 12px;
  margin-bottom: 0;
  font-size: 14px;
  font-weight: 500;
  line-height: 1.428571429;
  text-align: center;
  vertical-align: middle;
  cursor: pointer;
  border: 1px solid transparent;
  border-radius: 4px;
  white-space: nowrap;
  -webkit-transition: "border-color background-color" 0.3s ease;
  -moz-transition: "border-color background-color" 0.3s ease;
  -o-transition: "border-color background-color" 0.3s ease;
  transition: "border-color background-color" 0.3s ease;
  background-color: $color_btn_bg;
  color: $color_btn_text;
}
