  outline: thin dotted #333;
  outline: 5px auto -webkit-focus-ring-color;
  outline-offset: -2px;
  outline: 0;
  -webkit-box-shadow: 0 0 8px rgba(203,222,61, .6);
  -moz-box-shadow: 0 0 8px rgba(203,222,61, .6);
