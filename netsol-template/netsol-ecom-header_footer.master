<%@ Master Language="C#" MasterPageFile="~/netsol-template/netsol-ecom-site.master" AutoEventWireup="true" CodeFile="netsol-ecom-header_footer.master.cs" Inherits="netsol_template_netsol_ecom_header_footer" %>

<asp:Content ID="Content1" ContentPlaceHolderID="head_top" Runat="Server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="head_bottom" Runat="Server">
</asp:Content>
<asp:Content ID="Content3" ContentPlaceHolderID="ns_header" Runat="Server">
	
<style>
<!--
.prod-detail-sale-label, .prod-detail-sale-value {font-size: 13px; color: #000000;} .product-list-save {display: none !important;}
-->
</style>

	<div id="mastHead">
		<a id="ccLogo" class="imr" href="/">
		<img alt="C.Claire Custom Embroidery" src="/themes/default-1/images/layout/mastHead_cClaire-Logo.png" width="245" height="97" /></a>
		<div id="utilityNav">
			<ul style="margin-right: 10px" id="accountTools" class="listLink hor">
				<ns:choose>
        <ns:when condition="User.IsLoggedIn">
          
<li id="userWelcome"><a href="/account.aspx" rel="nofollow">Welcome,&nbsp;
              <%
              User.FirstName
              %>
              </a> </li>
          
<li>
            <ns:login layout="horizontal" logouttext="log out"></ns:login><br />
            </li>
          
<li><a href="/account.aspx">my account</a> </li>
          
<li><a href="/cart.aspx">my bag</a> 
            <ns:otherwise>
              
<li id="userWelcome">Welcome,&nbsp;Guest </li>
              
<li>
                <ns:login layout="horizontal" logouttext="Logout"></ns:login><br />
                
                <!--
                <li><a href="/account.aspx">my account</a></li><li><a href="/cart.aspx">my bag</a></li>
                -->
                </ns:choose>
				</ns:otherwise></ns:otherwise></ns:when></ns:choose>
			</ul>
			<div style="margin-right: 10px" id="utilSearch">
				<ns:search customtext="keyword or item #" type="custom"></ns:search>
			</div>
			<ul id="extraCheckout" class="listLink hor">
				<li><a class="input checkout sm" href="/cart.aspx">checkout</a> </li>
			</ul>
		</div>
		<div id="mainNav" class="&lt;%Category.Name%&gt;">
			<ul class="listLink hor">
				<li id="accessories"><a href="/accessories/default.aspx"><em>Accessories</em></a> </li>
				<li id="linens"><a href="/linens/default.aspx"><em>Linens</em></a> </li>
				<li id="apparel"><a href="/apparel/default.aspx"><em>Apparel</em></a> </li>
				<li id="baby"><a href="/baby/default.aspx"><em>Baby</em></a> </li>
				<li id="drinkware"><a href="/drinkware/default.aspx"><em>Drinkware</em></a> </li>
				<li id="cart"><a href="/cart.aspx"><em>Shopping Bag
					<ns:choose>
              <ns:when condition="Cart.Count != 0">( 
                <%
                Cart.Count
                %>
                )</ns:when></ns:choose>
					| Checkout</em></a>
					<!--
        <li><a href="/business-embroidery/default.aspx"><em>Embroidery for Businesses<em></a></li>
        -->
				</li>
			</ul>
		</div>
	</div>
	<!--
END MASTHEAD
-->

</asp:Content>
<asp:Content ID="Content4" ContentPlaceHolderID="ns_columns" Runat="Server">
	
<tr>
	<td id="ctl00_leftColumn" class="page-column-left">
		<asp:ContentPlaceHolder id="ns_columns_left-column" runat="server">
        
		</asp:ContentPlaceHolder>	
	</td> <!--/ #ctl00_leftColumn -->
	<td id="ct100_centerColumn" class="page-column-center">
		<table cellspacing="0" cellpadding="0" border="0" id="ctl00_columns" class="page-body-columns">
			<tbody>
				<tr>
					<td id="ctl00_leftColumn" class="page-column-left">
						<asp:ContentPlaceHolder id="ns_columns_center_left-column" runat="server">
        
						</asp:ContentPlaceHolder>
					</td> <!--/ centerColumn #ct100_leftColumn -->
					<td id="ctl00_centerColumn" class="page-column-center">
						<asp:ContentPlaceHolder id="ns_columns_center_center_page-body" runat="server">
        
						</asp:ContentPlaceHolder>
					</td> <!--/ centerColumn #ct100_centerColumn -->
				</tr>
			</tbody>
		</table> <!--/ centerColumn #ct100_columns -->
	</td>
</tr>

</asp:Content>
<asp:Content ID="Content5" ContentPlaceHolderID="ns_footer" Runat="Server">
	
<!--
TRIPLE FEATURED BOXES
-->
